<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%> <%	String path = request.getContextPath();	String basePath = request.getScheme() + "://"			+ request.getServerName() + ":" + request.getServerPort()			+ path + "/"; %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%><!DOCTYPE html><html>  <head>    <c:choose>    	<c:when test="${user.userTypeId==4}">    		<title>${loginBean.eName} - Home</title>    	</c:when>    	<c:otherwise><title>${loginBean.cName} - 首页</title></c:otherwise>    </c:choose>    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport" />    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />    <base href="<%=basePath%>" />  </head>  <body class="contrast-red">  <%@ include file="/common/res.jsp" %>    <!-- 引入header头部 -->    <%@ include file="/common/header.jsp" %>    <!-- 结束header头部 -->
     <div id="wrapper">      <!-- 引入左侧导航栏 -->      <%@ include file="/common/nav.jsp" %>      <!-- 结束左侧导航栏 -->      <section id="content">        <div class="container-fluid">          <div class="row-fluid" id="content-wrapper">            <div class="span12">                          <%@ include file="/common/staticPage.jsp" %>                                       <!-- 签证时间 -->              <input name="visaDueDate" type="hidden" value="${invalidTime}" />             <!-- 签证时间 -->                         </div>          </div>        </div>      </section>    </div>  </body></html>